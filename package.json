    "style-loader": "^1.1.3",
    "webpack": "4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "3.9.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "verbose": true,
    "rootDir": "./",
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "unmockedModulePathPatterns": [
      "react",
      "react-dom",
      "react-addons-test-utils",
      "enzyme"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/ext-react/packages/test-helpers/css-stub.js",
      "app-globals": "<rootDir>/src/Globals.js",
      "app-localization": "<rootDir>/src/localization/default.js",
      "@atg/utilities": "<rootDir>/ext-react/packages/utilities/index.js",
      "@atg/agent-scheduling": "<rootDir>/ext-react/packages/agent-scheduling/index.js"
    },
    "setupFiles": [
      "<rootDir>/ext-react/packages/test-helpers/setup.js"
    ]
  }
